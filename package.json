{
  "name": "@alferpal/change-this-please",
  "version": "1.0.0",
  "description": "change-this-please",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prebuild": "npm-run-all -p --silent clean lint",
    "prerelease": "npm t --silent",
    "prestart": "npm run build --silent",
    "pretest": "npm run build --silent",
    "release": "standard-version -s",
    "start": "node dist/server.js | pino-colada",
    "test": "jest",
    "updateCOC": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "updateContrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alferpal/change-this-please.git"
  },
  "homepage": "https://github.com/alferpal/change-this-please",
  "bugs": "https://github.com/alferpal/change-this-please/issues",
  "keywords": [],
  "author": {
    "email": "alferpal@gmail.com",
    "name": "Alex Ferrando",
    "twitter": "alferpal",
    "url": "https://github.com/alferpal"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "~7.4.3",
    "@hapi/hapi": "~18.3.2",
    "blipp": "~4.0.0",
    "bluebird": "~3.5.5",
    "glob": "~7.1.4",
    "hapi-pino": "~6.0.2",
    "lodash": "~4.17.15",
    "make-promises-safe": "~5.0.0",
    "pino": "~5.13.2",
    "pino-colada": "~1.4.5"
  },
  "devDependencies": {
    "@types/bluebird": "~3.5.27",
    "@types/chai": "~4.2.0",
    "@types/glob": "~7.1.1",
    "@types/hapi__boom": "~7.4.1",
    "@types/hapi__hapi": "~18.2.5",
    "@types/jest": "~24.0.17",
    "@types/lodash": "~4.14.136",
    "@types/pino": "~5.8.9",
    "chai": "~4.2.0",
    "commitizen": "~4.0.3",
    "concurrently": "~4.1.2",
    "cz-conventional-changelog": "~3.0.2",
    "husky": "~3.0.3",
    "jest": "~24.9.0",
    "npm-run-all": "~4.1.5",
    "onchange": "~6.0.0",
    "rimraf": "~3.0.0",
    "standard-version": "~7.0.0",
    "ts-jest": "~24.0.2",
    "tslint": "~5.18.0",
    "tslint-config-airbnb": "~5.11.1",
    "typescript": "~3.5.3",
    "weallbehave": "~1.2.0",
    "weallcontribute": "~1.0.9"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
