{
  "name": "@alferpal/change-this-please",
  "version": "1.0.0",
  "description": "change-this-please",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prebuild": "npm-run-all -p --silent clean lint",
    "prerelease": "npm t --silent",
    "prestart": "npm run build --silent",
    "pretest": "npm run build --silent",
    "release": "standard-version -s",
    "start": "node dist/server.js | pino-colada",
    "test": "jest",
    "updateCOC": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "updateContrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alferpal/change-this-please.git"
  },
  "homepage": "https://github.com/alferpal/change-this-please",
  "bugs": "https://github.com/alferpal/change-this-please/issues",
  "keywords": [],
  "author": {
    "email": "alferpal@gmail.com",
    "name": "Alex Ferrando",
    "twitter": "alferpal",
    "url": "https://github.com/alferpal"
  },
  "license": "MIT",
  "dependencies": {
    "blipp": "~3.1.1",
    "bluebird": "~3.5.3",
    "boom": "~7.3.0",
    "glob": "~7.1.3",
    "hapi": "~17.8.1",
    "hapi-pino": "~5.2.0",
    "lodash": "~4.17.11",
    "make-promises-safe": "~4.0.0",
    "pino": "~5.10.3",
    "pino-colada": "~1.4.4"
  },
  "devDependencies": {
    "@types/bluebird": "~3.5.25",
    "@types/boom": "~7.2.1",
    "@types/chai": "~4.1.7",
    "@types/glob": "~7.1.1",
    "@types/hapi": "~17.8.2",
    "@types/jest": "~23.3.10",
    "@types/lodash": "~4.14.119",
    "@types/pino": "~5.8.3",
    "chai": "~4.2.0",
    "commitizen": "~3.0.5",
    "concurrently": "~4.1.0",
    "cz-conventional-changelog": "~2.1.0",
    "husky": "~1.3.0",
    "jest": "~23.6.0",
    "npm-run-all": "~4.1.5",
    "onchange": "~5.2.0",
    "rimraf": "~2.6.2",
    "standard-version": "~4.4.0",
    "ts-jest": "~23.10.5",
    "tslint": "~5.12.0",
    "tslint-config-airbnb": "~5.11.1",
    "typescript": "~3.2.2",
    "weallbehave": "~1.2.0",
    "weallcontribute": "~1.0.8"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
