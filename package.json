{
  "name": "@alferpal/change-this-please",
  "version": "1.0.0",
  "description": "change-this-please",
  "scripts": {
    "build": "tsc --build tsconfig-src.json",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "lint": "eslint --ext .ts src",
    "prebuild": "npm-run-all -p clean lint",
    "prerelease": "npm t",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "release": "standard-version -s",
    "start": "node dist/server.js | pino-colada",
    "test": "jest",
    "updateCOC": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "updateContrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alferpal/change-this-please.git"
  },
  "homepage": "https://github.com/alferpal/change-this-please",
  "bugs": "https://github.com/alferpal/change-this-please/issues",
  "keywords": [],
  "author": {
    "email": "alferpal@gmail.com",
    "name": "Alex Ferrando",
    "twitter": "alferpal",
    "url": "https://github.com/alferpal"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "~8.0.1",
    "@hapi/hapi": "~18.4.0",
    "blipp": "~4.0.1",
    "bluebird": "~3.7.2",
    "glob": "~7.1.6",
    "hapi-pino": "~6.3.0",
    "lodash": "~4.17.15",
    "make-promises-safe": "~5.1.0",
    "pino": "~5.15.0",
    "pino-colada": "~1.5.0"
  },
  "devDependencies": {
    "@types/bluebird": "~3.5.29",
    "@types/glob": "~7.1.1",
    "@types/hapi__hapi": "~18.2.6",
    "@types/jest": "~24.0.23",
    "@types/lodash": "~4.14.149",
    "@types/pino": "~5.14.0",
    "@typescript-eslint/eslint-plugin": "~1.13.0",
    "@typescript-eslint/parser": "~1.13.0",
    "commitizen": "~4.0.3",
    "concurrently": "~5.0.1",
    "cz-conventional-changelog": "~3.0.2",
    "eslint": "~6.7.2",
    "eslint-config-airbnb-typescript": "~6.2.0",
    "eslint-plugin-import": "~2.19.1",
    "husky": "~3.1.0",
    "jest": "~24.9.0",
    "npm-run-all": "~4.1.5",
    "onchange": "~6.1.0",
    "rimraf": "~3.0.0",
    "standard-version": "~7.0.1",
    "ts-jest": "~24.2.0",
    "typescript": "~3.7.3",
    "weallbehave": "~1.2.0",
    "weallcontribute": "~1.0.9"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
